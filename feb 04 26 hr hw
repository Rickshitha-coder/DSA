5. Mathematical Algorithms - Odd Count Detector in a Given Range

import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner (System.in);
        int odd_c=0;
        int start=sc.nextInt();
        int end=sc.nextInt();
        for(int i=start;i<=end;i++)
        {
            if(i%2==1)
            {
                odd_c+=1;
            }
        }
        System.out.print(odd_c);
    }
}

